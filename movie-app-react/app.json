{
  "expo": {
    "name": "movie-app-react",
    "slug": "movie-app-react",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#201E26"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#201E26"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photoLibraryPermission": "Allow Movie Zone to access your photo library.",
          "saveToLibraryPermission": "Allow Movie Zone to save your photos to your photo library.",
          "isAccessMediaLibraryRequired": "true"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Movie Zone to access your camera.",
          "mediaLibraryPermission": "Allow Movie Zone to access your media library."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to upload your profile picture."
        }
      ]
    ]
  }
}